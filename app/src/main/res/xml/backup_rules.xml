<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!--
    Backup all files except those in the cache directory
    -->
    <include domain="file" path="." />
    <exclude domain="file" path="cache" />
    <exclude domain="database" path="." />
    <exclude domain="sharedpref" path="." />
</full-backup-content>